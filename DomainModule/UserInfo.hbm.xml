<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="DomainModule.UserInfo,DomainModule" table="TB_UserInfo">
    <id name="Id" column="ID" type="String">
      <generator class="assigned" />
    </id>
    <property name="LoginName" column="LoginName" type="String" length="50"/>
    <property name="Password" column="Password" type="String" length="50"/>
    <property name="UserName" column="UserName" type="String" length="50"/>
    <property name="AmwayCardId" column="AmwayCardId" type="String" length="50"/>
    <property name="Address" column="Address" type="String" length="50"/>
    <property name="IdentityNumber" column="IdentityNumber" type="String" length="50"/>
    <property name="Phone" column="Phone" type="String" length="50"/>
    <property name="StartTime" column="StartTime" type="DateTime"/>
    <property name="EndTime" column="EndTime" type="DateTime"/>

    <property name="IsDel" column="IsDel" type="Boolean"/>
    <property name="CreateTime" column="CreateTime" type="DateTime"/>
  </class>
</hibernate-mapping>